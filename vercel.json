{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/v1(.*)",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "public/dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "public/dist/$1"
      }
    ]
  }